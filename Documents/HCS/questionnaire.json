{
    "basicInfo": {
      "title": "Basic Information",
      "questions": [
        {
          "id": "age-group",
          "type": "select",
          "label": "Age Group:",
          "options": [
            {"value": "18-24", "text": "18-24 years"},
            {"value": "25-34", "text": "25-34 years"},
            {"value": "35-44", "text": "35-44 years"},
            {"value": "45-54", "text": "45-54 years"},
            {"value": "55+", "text": "55 years and above"},
            {"value": "prefer-not-to-say", "text": "Prefer not to say"}
          ]
        },
        {
          "id": "gender",
          "type": "select",
          "label": "Gender:",
          "options": [
            {"value": "male", "text": "Male"},
            {"value": "female", "text": "Female"},
            {"value": "other", "text": "Other"},
            {"value": "prefer-not-to-say", "text": "Prefer not to say"}
          ]
        },
        {
          "id": "music-background",
          "type": "select",
          "label": "Your Music Background:",
          "options": [
            {"value": "professional", "text": "Professional Musician/Music Enthusiast"},
            {"value": "regular", "text": "Listen to Music Regularly"},
            {"value": "occasional", "text": "Listen to Music Occasionally"},
            {"value": "rarely", "text": "Rarely Listen to Music"}
          ]
