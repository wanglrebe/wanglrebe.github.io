        {
          "id": "auth-methods",
          "type": "checkbox",
          "label": "Authentication Methods You Usually Use (Check all that apply):",
          "options": [
            {"id": "auth-password", "value": "password", "text": "Alphanumeric Passwords"},
            {"id": "auth-pin", "value": "pin", "text": "PIN Codes"},
